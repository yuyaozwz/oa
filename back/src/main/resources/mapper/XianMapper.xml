<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.exrick.xboot.modules.crm.mapper.XianMapper">
    <select id="findShiByShengShi" resultType="cn.exrick.xboot.modules.crm.entity.Xian">
        SELECT * from t_xian x
        where x.sheng = #{sheng} and x.city = #{city};
    </select>
</mapper>