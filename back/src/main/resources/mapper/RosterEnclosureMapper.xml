<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.exrick.xboot.modules.your.mapper.RosterEnclosureMapper">

    <select id="findById" resultType="cn.exrick.xboot.modules.your.entity.RosterEnclosure">
      SELECT * FROM t_roster_enclosure
      WHERE id = #{id}
    </select>

    <select id="findByRosterId" resultType="cn.exrick.xboot.modules.your.entity.RosterEnclosure">
      SELECT * FROM t_roster_enclosure
      WHERE roster_id = #{rosterId}
    </select>

</mapper>