<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.exrick.xboot.modules.your.mapper.DocumentTemplateMapper">
    <select id="findByIdZwz" resultType="cn.exrick.xboot.modules.your.entity.DocumentTemplate">
        SELECT * from t_document_template
        where id = #{id}
    </select>

    <select id="findAllHou" resultType="String">
      SELECT distinct hou
      FROM t_document_template
    </select>
</mapper>